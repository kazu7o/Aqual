{
  "expo": {
    "name": "Aqual",
    "description": "This app makes useful to manage your aqualium.",
    "slug": "Aqual",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kztmr.aqual"
    },
    "android": {
      "package": "com.kztmr.aqual"
    }
  }
}
